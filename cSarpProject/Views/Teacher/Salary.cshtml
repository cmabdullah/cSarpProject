@{
  Layout = "_Layout_teacher";
}
﻿

@model cSarpProject.Models.EClassSalaryT



<div class="col-sm-12">
    <div class="table-responsive">
        <table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Basic</th>
                    <th>House Rent</th>
                    <th>Medical</th>
                    <th>Bonus</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var data in Model.SalaryFT)
                {
                    <tr title="Click Here to See Details">
                        <td class="showDetails pointer">@data.Id</td>
                        <td class="showDetails pointer">@data.Basic</td>
                        <td class="showDetails pointer">@data.HouseRent</td>
                    <td class="showDetails pointer">@data.Medical</td>
                    <td class="showDetails pointer">@data.Bonus</td>

    

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>