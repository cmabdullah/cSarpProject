@{
  Layout = "_Layout_accounce";
}
﻿
@model cSarpProject.Models.EClassLeave



<div class="col-sm-12">
    <div class="table-responsive">
        <table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Casual Leave</th>
                    <th>Study Leave</th>
                    <th>Medical</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var data in Model.LeaveF)
                {
                    <tr title="Click Here to See Details">
                        <td class="showDetails pointer">@data.Id</td>
                        <td class="showDetails pointer">@data.CasualLeave</td>
                        <td class="showDetails pointer">@data.StudyLeave</td>
                    <td class="showDetails pointer">@data.Medical</td>

    

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>