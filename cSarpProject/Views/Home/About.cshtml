
@model cSarpProject.Models.PhotoGallary1



<div class="col-sm-12">
    <div class="table-responsive">
        <table id="table" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Department</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var data in Model.StudentAll1)
                {
                    <tr title="Click Here to See Details">
                        <td class="showDetails pointer">@data.Id</td>
                        <td class="showDetails pointer">@data.Name</td>
                        <td class="showDetails pointer">@data.Department</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>